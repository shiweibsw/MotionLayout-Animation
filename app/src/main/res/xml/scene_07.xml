<?xml version="1.0" encoding="utf-8"?>
<MotionScene xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto">
    <ConstraintSet android:id="@+id/start">
        <Constraint
            android:id="@+id/m1"
            android:layout_width="0dp"
            android:layout_height="56dp"
            android:layout_marginLeft="20dp"
            android:layout_marginRight="20dp"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintLeft_toLeftOf="parent"
            app:layout_constraintRight_toRightOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintVertical_bias="0.2" />
        <Constraint
            android:id="@+id/m2"
            android:layout_width="0dp"
            android:layout_height="56dp"
            android:layout_marginLeft="20dp"
            android:layout_marginRight="20dp"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintLeft_toLeftOf="parent"
            app:layout_constraintRight_toRightOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintVertical_bias="0.4" />
        <Constraint
            android:id="@+id/m3"
            android:layout_width="0dp"
            android:layout_height="56dp"
            android:layout_marginLeft="20dp"
            android:layout_marginRight="20dp"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintLeft_toLeftOf="parent"
            app:layout_constraintRight_toRightOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintVertical_bias="0.6" />
        <Constraint
            android:id="@+id/m4"
            android:layout_width="0dp"
            android:layout_height="56dp"
            android:layout_marginLeft="20dp"
            android:layout_marginRight="20dp"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintLeft_toLeftOf="parent"
            app:layout_constraintRight_toRightOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintVertical_bias="0.8" />
    </ConstraintSet>
    <ConstraintSet android:id="@+id/end">
        <Constraint
            android:id="@+id/m1"
            android:layout_width="0dp"
            android:layout_height="4dp"
            android:layout_marginLeft="16dp"
            android:layout_marginRight="16dp"
            app:layout_constraintBottom_toBottomOf="@id/fb"
            app:layout_constraintLeft_toLeftOf="@id/fb"
            app:layout_constraintRight_toRightOf="@id/fb"
            app:layout_constraintTop_toTopOf="@id/fb"
            app:layout_constraintVertical_bias="0.3" />
        <Constraint
            android:id="@+id/m2"
            android:layout_width="0dp"
            android:layout_height="4dp"
            android:layout_marginLeft="16dp"
            android:layout_marginRight="16dp"
            app:layout_constraintBottom_toBottomOf="@id/fb"
            app:layout_constraintLeft_toLeftOf="@id/fb"
            app:layout_constraintRight_toRightOf="@id/fb"
            app:layout_constraintTop_toTopOf="@id/fb"
            app:layout_constraintVertical_bias="0.45" />
        <Constraint
            android:id="@+id/m3"
            android:layout_width="0dp"
            android:layout_height="4dp"
            android:layout_marginLeft="16dp"
            android:layout_marginRight="16dp"
            app:layout_constraintBottom_toBottomOf="@id/fb"
            app:layout_constraintLeft_toLeftOf="@id/fb"
            app:layout_constraintRight_toRightOf="@id/fb"
            app:layout_constraintTop_toTopOf="@id/fb"
            app:layout_constraintVertical_bias="0.6" />
        <Constraint
            android:id="@+id/m4"
            android:layout_width="0dp"
            android:layout_height="4dp"
            android:layout_marginLeft="16dp"
            android:layout_marginRight="16dp"
            app:layout_constraintBottom_toBottomOf="@id/fb"
            app:layout_constraintLeft_toLeftOf="@id/fb"
            app:layout_constraintRight_toRightOf="@id/fb"
            app:layout_constraintTop_toTopOf="@id/fb"
            app:layout_constraintVertical_bias="0.75" />
    </ConstraintSet>

    <Transition
        app:constraintSetEnd="@id/end"
        app:constraintSetStart="@id/start"
        app:duration="600">
        <OnClick
            app:clickAction="toggle"
            app:targetId="@id/fb" />
        <KeyFrameSet>

            <KeyPosition
                app:framePosition="10"
                app:motionTarget="@id/m1"
                app:percentY="-0.1" />

            <KeyPosition
                app:framePosition="10"
                app:motionTarget="@id/m2"
                app:percentY="-0.1" />

            <KeyPosition
                app:framePosition="10"
                app:motionTarget="@id/m3"
                app:percentY="-0.1" />

            <KeyPosition
                app:framePosition="10"
                app:motionTarget="@id/m4"
                app:percentY="-0.1" />
        </KeyFrameSet>
    </Transition>


</MotionScene>